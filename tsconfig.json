{
	"compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
		"module": "esnext",
		"allowJs": true,
		"jsx": "react",
		"noEmit": true,
		"isolatedModules": true,
		"strict": false,
		"moduleResolution": "node",
		"baseUrl": ".",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
    // "sourceMap": true,
    // "checkJs": true
	},
	"include": [ "assets/js/**/*" ]
}
